{
  "name": "chrome-aws-anything",
  "version": "0.0.1",
  "description": "Chrome aws anything",
  "main": "gulpfile.babel.js",
  "scripts": {
    "build": "npm run build:clean && gulp build",
    "watch": "gulp watch",
    "release": "npm run build && zip -r build.zip build",
    "build:clean": "ls build 2> /dev/null 1>&2 && rm -rf build/*"
  },
  "dependencies": {
    "react": "^0.14.6",
    "react-dom": "^0.14.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.1",
    "babel-plugin-transform-react-jsx": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "crx": "^3.2.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "gulp": "^3.9.0",
    "gulp-sass": "^2.3.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0"
  }
